package mycalculator.utility;

public class ViewRegenerate {

	public ViewRegenerate(Register register){

		String regA = register.getRegA();
		String regB = register.getRegB();
		String symbol = register.getOperator();

		calculation.setText(regA + symbol + regB + " ");
		if(regB != ""){
			result.setText(regB + " ");
		}else if(regA != ""){
			result.setText(regA + " ");
		}else{
			result.setText("0 ");
		}
		if(mode == 0){
			regA = "";
		}
	}


}
